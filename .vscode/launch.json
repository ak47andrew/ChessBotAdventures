{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: GUI",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/core/Chess-Challenge/Chess-Challenge.csproj"
        },
        {
            "name": "C#: UCI",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/core/Chess-Challenge/bin/Debug/net6.0/Chess-Challenge.dll",
            "args": [
                "uci",
                "MyBot"
            ],
            "cwd": "${workspaceFolder}/core",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Python: UCI",
            "type": "debugpy",
            "request": "launch",
            "program": "tournament.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/python"
        },
        {
            "name": "Python: Test",
            "type": "debugpy",
            "request": "launch",
            "program": "test.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/python"
        },
    ]
}